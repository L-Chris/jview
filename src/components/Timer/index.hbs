<ul class="ek-timer clearfix text-center">
  {{#each @root}}
  <li class="ek-timer-item" data-type="{{type}}">
    <ul class="clearfix">
      {{#each data as |number|}}
      <li class="ek-timer-number">{{number}}</li>
      {{/each}}
      <li class="ek-timer-desc">{{label}}</li>
    </ul>
  </li>
  {{/each}}
</ul>